    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "java",
            "name": "Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "DataFlowEngineApplication",
            "request": "launch",
            "mainClass": "com.tool.Data.Flow.Engine.DataFlowEngineApplication",
            "projectName": "Data-Flow-Engine"
        },
        
        {
            "name": "Launch Chrome",
            "request": "launch",
            "type": "chrome",
            "url": "http://localhost:8080",
            "webRoot": "${workspaceFolder}"
        }

    ]
}